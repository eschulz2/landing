

<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>First Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Link</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.id %></td>
        <td><%= contact.first_name %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.email %></td>
        <td><%= contact.link %></td>

      </tr>
    <% end %>
  </tbody>
</table>